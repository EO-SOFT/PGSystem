<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
    <class name="ManufactureUsers" table="manufacture_users">
        <id name="id">
            <!--  PostgreSQL generator for a sequence which is named manufacture_users_id_seq
            -->
            <generator class="sequence">
                <param name="sequence">manufacture_users_id_seq</param>
            </generator>
        </id>
        <property column="first_name" name="firstName"/>
        <property column="last_name" name="lastName"/>        
        
        <property column="create_time" name="createTime" type="java.sql.Timestamp"/>
        <property column="create_id" name="createId" />
        <property column="write_time" name="writeTime" type="java.sql.Timestamp"/>
        <property column="write_id" name="writeId" />
        <property column="login_time" name="loginTime" type="java.sql.Timestamp"/>
        <property column="login" name="login" type="string" unique="true" />    
        <property column="password" name="password" />
        <property column="access_level" name="accessLevel"/>
        <property column="active" name="active" />
        <property column="harness_type" name="harnessType" />
                
    </class>
</hibernate-mapping>
